<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <customSettingsVisibility>Protected</customSettingsVisibility>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>IssuedToUserId__c</fullName>
        <defaultValue>$User.Id</defaultValue>
        <description>Holds the SalesForce User ID for the person the token was issued to.   This is the user who completed the &apos;Connect to QuickBooks&apos; wizard</description>
        <externalId>false</externalId>
        <label>User ID</label>
        <length>20</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Realm__c</fullName>
        <externalId>false</externalId>
        <label>Realm</label>
        <length>30</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>TokenSecret__c</fullName>
        <description>The token&apos;s secret key.  This will be blank while the OAuth connect process is under way</description>
        <externalId>false</externalId>
        <label>Token secret</label>
        <length>130</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Token__c</fullName>
        <caseSensitive>false</caseSensitive>
        <externalId>false</externalId>
        <label>Token</label>
        <length>130</length>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>true</unique>
    </fields>
    <fields>
        <fullName>UnauthorizedErrorCount__c</fullName>
        <defaultValue>0</defaultValue>
        <description>Number of times we have encountered a 401 error when using this token.  Gives an indication that it may have been invalidated</description>
        <externalId>false</externalId>
        <label>Unauthorized Error Count</label>
        <precision>8</precision>
        <required>true</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>ValidToDate__c</fullName>
        <externalId>false</externalId>
        <label>Valid to</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
    <label>Quickbooks API Token</label>
</CustomObject>
