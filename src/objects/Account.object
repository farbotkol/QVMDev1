    <validationRules>
        <fullName>BSB_Number_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND(  AND( LEN( BSB_Number__c )  &gt; 0, LEN( BSB_Number__c ) &lt;= 7 ), (OR(NOT(ISNUMBER(LEFT( BSB_Number__c , 3)) ),  NOT( ISNUMBER( RIGHT( BSB_Number__c , 3) ) ),  NOT( MID( BSB_Number__c , 4, 1)  =  &apos;-&apos; )    )))</errorConditionFormula>
        <errorDisplayField>BSB_Number__c</errorDisplayField>
        <errorMessage>Please enter a valid BSB number [XXX-XXX where X is a number]</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cant_close_with_open_Account_Businesses</fullName>
        <active>true</active>
        <description>For an account to be closed it must have all account business be closed</description>
        <errorConditionFormula>IF( TEXT(Trader_Status__c) = &quot;Closed&quot; &amp;&amp;  Open_Account_Businesses__c  &gt; 0 , True, False)</errorConditionFormula>
        <errorMessage>For an account to be closed all related account businesses MUST be closed</errorMessage>
    </validationRules>
